    "node": ">=16.0.0 <17.0.0"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "pug": "^3.0.2",
    "useid-eservice-sdk": "1.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "nodemon": "^2.0.16"
  }
}
